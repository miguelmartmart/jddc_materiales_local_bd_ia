{
    "tables": {
        "ALMACEN": {
            "category": "inventario",
            "record_count": 10,
            "description": "Almacenes de la empresa",
            "primary_keys": [
                "CODIGO"
            ],
            "columns": {
                "CODIGO": "INTEGER - Código único del almacén",
                "DESCRIPCION": "VARCHAR(40) - Nombre o descripción del almacén"
            },
            "consultas_comunes": [
                "listar almacenes: SELECT * FROM ALMACEN",
                "buscar almacén por nombre: WHERE DESCRIPCION LIKE '%X%'"
            ]
        },
        "ARTICULO": {
            "category": "productos",
            "record_count": 1000,
            "description": "Productos/artículos del inventario",
            "primary_keys": [
                "CODIGO"
            ],
            "columns": {
                "CODIGO": "VARCHAR(20) - Código único del artículo",
                "NOMBRE": "VARCHAR(100) - Nombre/descripción del producto",
                "PRECIOCMPONDERADO": "DECIMAL(10,2) - Precio de compra ponderado",
                "STOCK": "DECIMAL(10,2) - Cantidad en stock/inventario",
                "CODFAMILIA": "VARCHAR(10) - Código de familia/categoría",
                "CODMARCA": "VARCHAR(10) - Código de marca",
                "OBSERVACIONES": "BLOB - Observaciones del artículo"
            },
            "consultas_comunes": [
                "productos más caros: ORDER BY PRECIOCMPONDERADO DESC FIRST 10",
                "productos con stock: WHERE STOCK > 0",
                "productos por nombre: WHERE NOMBRE LIKE '%texto%'"
            ]
        },
        "CLIENTE": {
            "category": "clientes",
            "record_count": 500,
            "description": "Clientes de la empresa",
            "primary_keys": [
                "CODIGO"
            ],
            "columns": {
                "CODIGO": "VARCHAR(20) - Código único del cliente",
                "NOMBRE": "VARCHAR(100) - Nombre o razón social",
                "NIF": "VARCHAR(20) - NIF/CIF",
                "DIRECCION": "VARCHAR(200) - Dirección postal",
                "TELEFONO": "VARCHAR(20) - Teléfono de contacto",
                "EMAIL": "VARCHAR(100) - Email de contacto",
                "DESCUENTO": "DECIMAL(5,2) - Descuento aplicable"
            },
            "consultas_comunes": [
                "buscar cliente por nombre: WHERE NOMBRE LIKE '%X%'",
                "clientes con descuento: WHERE DESCUENTO > 0"
            ]
        },
        "DOCCAB": {
            "category": "ventas",
            "record_count": 69927,
            "description": "Documentos de venta (facturas, presupuestos, albaranes)",
            "primary_keys": [
                "NUMERO"
            ],
            "columns": {
                "NUMERO": "INTEGER - Número de documento",
                "TIPO": "INTEGER - Tipo de documento: 0=Presupuesto, 11=Albarán, 12=Pedido, 51=Certificado obra, 21=Transferencia, 13=Factura, 2=Orden trabajo SAT, 3=Abono, 61=Recibo, 10=Contrato",
                "SERIE": "VARCHAR(10) - Serie del documento",
                "FECHA": "DATE - Fecha de emisión",
                "CODCLIENTE": "INTEGER - Código del cliente",
                "IMPORTETOTAL": "DECIMAL(12,2) - Importe total del documento",
                "IMPORTEBASE": "DECIMAL(12,2) - Importe base imponible",
                "IMPORTEIVA": "DECIMAL(12,2) - Importe del IVA",
                "DESCRIPCION": "VARCHAR(200) - Descripción del documento",
                "OBSERVACIONES": "BLOB - Observaciones"
            },
            "consultas_comunes": [
                "documentos por cliente: WHERE CODCLIENTE = X",
                "documentos por fecha: WHERE FECHA BETWEEN X AND Y",
                "total facturado: SUM(IMPORTETOTAL)",
                "documentos este mes: WHERE EXTRACT(MONTH FROM FECHA) = EXTRACT(MONTH FROM CURRENT_DATE)"
            ]
        },
        "PROVEEDOR": {
            "category": "proveedores",
            "record_count": 200,
            "description": "Proveedores de productos",
            "primary_keys": [
                "CODIGO"
            ],
            "columns": {
                "CODIGO": "VARCHAR(20) - Código único del proveedor",
                "NOMBRE": "VARCHAR(100) - Nombre o razón social",
                "NIF": "VARCHAR(20) - NIF/CIF",
                "CONTACTO": "VARCHAR(100) - Persona de contacto",
                "TELEFONO": "VARCHAR(20) - Teléfono"
            },
            "consultas_comunes": [
                "buscar proveedor: WHERE NOMBRE LIKE '%X%'"
            ]
        },
        "PEDIDO": {
            "category": "compras",
            "record_count": 1000,
            "description": "Pedidos de compra a proveedores",
            "primary_keys": [
                "NUMERO"
            ],
            "columns": {
                "NUMERO": "INTEGER - Número de pedido",
                "FECHA": "DATE - Fecha del pedido",
                "PROVEEDOR": "VARCHAR(20) - Código del proveedor",
                "TOTAL": "DECIMAL(12,2) - Importe total",
                "RECIBIDO": "CHAR(1) - Estado de recepción (S/N)"
            },
            "consultas_comunes": [
                "pedidos pendientes: WHERE RECIBIDO = 'N'",
                "pedidos por proveedor: WHERE PROVEEDOR = X"
            ]
        }
    }
}